    "typescript",
    "stateless"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/arm-compute": "^22.4.0",
    "@azure/arm-network": "^33.5.0",
    "@azure/identity": "^4.9.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
